<Window x:Class="FitTrack.RegisterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FitTrack"
        mc:Ignorable="d"
        Title="RegisterWindow" Height="300" Width="400">
    <Grid>
        <StackPanel Grid.Row="1" Orientation="Vertical" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="5,5,0,5">
            <TextBlock Text="Username:"/>
            <TextBox x:Name="UserName" Width="70" />
            <TextBlock Text="Password:"/>
            <TextBox x:Name="PassWord" Width="70"/>
            <ComboBox x:Name="comboBox" Margin="0,5,0,5">
            </ComboBox>
            <Button Name="Register" Content="Register" Click="Register_Click"/>

        </StackPanel>
    </Grid>
</Window>
